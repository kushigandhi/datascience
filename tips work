   "source": [
    "def view(x):\n",
    "    if x>0 and x<=3:\n",
    "        return'bad'\n",
    "    elif x>3 and x<=6:\n",
    "        return'good'\n",
    "    else:\n",
    "        return'excellent'\n",
    "        "
   ]
  },
