   "metadata": {},
   "outputs": [],
   "source": [
    "def grp(x):\n",
    "    if x<0 and x>=10:\n",
    "        return'0 -10'\n",
    "    elif x<20 and x>10:\n",
    "         return'10-20'\n",
    "    elif x<30 and x>20:\n",
    "         return'20-30'\n",
    "    elif x<40 and x>30:\n",
    "         return'30-40'\n",
    "    elif x<50 and x>50:\n",
    "         return'40-50'\n",
    "    else:\n",
    "        return'50-60'\n",
    "        "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 118,
